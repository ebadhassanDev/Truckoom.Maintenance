{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.AspNetCore": "Warning",
        "System": "Error"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithEnvironmentName",
      "WithMachineName"
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "None",
      "_Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "jwtPolicyName": "jwt",
  "Jwt": {
    "Secret": "localhost",
    "ValidIssuer": "dotnet-user-jwts",
    "LocalAuthIssuer": {
      "ValidAudiences": [
        "https://localhost:7105",
        "http://localhost:5271"
      ],
      "ValidIssuer": "local-auth"
    }
  }
}